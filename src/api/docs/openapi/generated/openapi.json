{
  "openapi": "3.0.0",
  "info": {
    "title": "Kanji Documentation",
    "version": "1.0.0"
  },
  "components": {
    "schemas": {
      "Kunyomi": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "kanji_id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "ja-Hira": {
            "type": "string",
            "description": "Kunyomi in hiragana",
            "example": "イチ"
          },
          "ja-Latn": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          }
        },
        "required": [
          "id",
          "kanji_id",
          "ja-Hira",
          "ja-Latn"
        ]
      },
      "Onyomi": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "kanji_id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "ja-Hira": {
            "type": "string",
            "description": "Onyomi in katakana",
            "example": "イチ"
          },
          "ja-Latn": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          }
        },
        "required": [
          "id",
          "kanji_id",
          "ja-Hira",
          "ja-Latn"
        ]
      },
      "Vocabulary": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "kanji_id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "word": {
            "type": "string",
            "description": "Vocabulary in katakana",
            "example": "イチ"
          },
          "kanji": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          },
          "romaji": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          },
          "image_url": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          },
          "id-ID": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          },
          "en-US": {
            "type": "string",
            "description": "Romanized Japanese",
            "example": "ichi"
          }
        },
        "required": [
          "id",
          "kanji_id",
          "word",
          "kanji",
          "romaji",
          "image_url",
          "id-ID",
          "en-US"
        ]
      },
      "Kanji": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "kanji": {
            "type": "string",
            "description": "Kanji's example",
            "example": "一"
          },
          "stroke": {
            "type": "number",
            "description": "Number of stroke",
            "example": 1
          },
          "level": {
            "type": "string",
            "example": "N5"
          }
        },
        "required": [
          "id",
          "kanji",
          "stroke",
          "level"
        ]
      },
      "Level": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "example": "Vjo1a910QP"
          },
          "name": {
            "type": "string",
            "example": "N5"
          }
        },
        "required": [
          "id",
          "name"
        ]
      }
    },
    "parameters": {}
  },
  "paths": {
    "/api/kanji": {
      "get": {
        "tags": [
          "Kanji"
        ],
        "parameters": [
          {
            "schema": {
              "type": "string",
              "nullable": true,
              "enum": [
                "N5",
                "N4",
                "N3",
                "N2",
                "N1"
              ]
            },
            "required": false,
            "name": "level",
            "in": "query"
          },
          {
            "schema": {
              "type": "string",
              "nullable": true
            },
            "required": false,
            "name": "id",
            "in": "query"
          }
        ],
        "responses": {
          "200": {
            "description": "Kanji list retrieved",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "allOf": [
                          {
                            "$ref": "#/components/schemas/Kanji"
                          },
                          {
                            "type": "object",
                            "properties": {
                              "kunyomi": {
                                "type": "array",
                                "items": {
                                  "$ref": "#/components/schemas/Kunyomi"
                                }
                              },
                              "onyomi": {
                                "type": "array",
                                "items": {
                                  "$ref": "#/components/schemas/Onyomi"
                                }
                              },
                              "vocabulary": {
                                "type": "array",
                                "items": {
                                  "$ref": "#/components/schemas/Vocabulary"
                                }
                              }
                            },
                            "required": [
                              "kunyomi",
                              "onyomi",
                              "vocabulary"
                            ]
                          }
                        ]
                      }
                    },
                    "error": {
                      "nullable": true
                    }
                  },
                  "required": [
                    "success"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "/api/level": {
      "get": {
        "tags": [
          "Kanji"
        ],
        "responses": {
          "200": {
            "description": "Post created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "success": {
                      "type": "boolean"
                    },
                    "message": {
                      "type": "string"
                    },
                    "data": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/Level"
                      }
                    },
                    "error": {
                      "nullable": true
                    }
                  },
                  "required": [
                    "success"
                  ]
                }
              }
            }
          }
        }
      }
    }
  }
}